on post-fs-data
    chown system system /system/etc/wifi/WCNSS_qcom_wlan_nv.bin

on boot
    chmod 0666 /dev/blfp
    chmod 0666 /dev/goodix_fp

service gx_fpd /vendor/bin/gx_fpd
    class late_start
    user system
    group system input

service btlfpserver /vendor/bin/btlfpserver
    class late_start
    user system
    group system input
